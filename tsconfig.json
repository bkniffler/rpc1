{
  "compilerOptions": {
    "module": "esnext",
    "declaration": true,
    "noImplicitAny": true,
    "removeComments": true,
    "noLib": false,
    "moduleResolution": "node",
    "target": "es5",
    "lib": ["es6", "dom"],
    "allowJs": false,
    "jsx": "react",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "declarationMap": true,
    "sourceMap": true,
    "importHelpers": true,
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.cache/typescript",
    "baseUrl": "./",
    "paths": {
      "rpc1": ["./src/core"],
      "rpc1-react": ["./src/react"],
      "rpc1-socket": ["./src/socket"],
      "rpc1-socket-server": ["./src/socket-server"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "lib"]
}
